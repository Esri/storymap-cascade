{
  "name": "@vimeo/player",
  "version": "2.6.2",
  "description": "Interact with and control an embedded Vimeo Player.",
  "license": "MIT",
  "author": "Brad Dougherty <brad@vimeo.com>",
  "contributors": [
    "Brad Dougherty <brad@vimeo.com> (https://brad.is)"
  ],
  "scripts": {
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean -s",
    "build": "cross-env BABEL_ENV=production node rollup.js",
    "lint": "eslint src/ test/",
    "test": "nyc --cache --reporter=text --reporter=html ava",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "watch:build": "npm run build -- --watch",
    "watch:lint": "chokidar 'src/**/*.js' 'test/**/*.js' -c 'eslint {path}'",
    "watch:test": "ava --watch",
    "watch": "run-s watch:**",
    "preversion": "npm test",
    "version": "npm run build && git add -A dist",
    "postversion": "git branch -f latest master",
    "push-release": "git push origin master latest && git push --tags"
  },
  "main": "dist/player.js",
  "module": "dist/player.es.js",
  "jsnext:main": "dist/player.es.js",
  "directories": {
    "lib": "src/"
  },
  "files": [
    "src",
    "dist"
  ],
  "repository": "vimeo/player.js",
  "bugs": "https://github.com/vimeo/player.js/issues",
  "keywords": [
    "vimeo",
    "player",
    "api",
    "iframe",
    "embed",
    "video",
    "javascript",
    "postMessage"
  ],
  "dependencies": {
    "es6-collections": "0.5.6",
    "native-promise-only": "0.8.1"
  },
  "devDependencies": {
    "@vimeo/eslint-config-player": "^4.0.1",
    "ava": "^0.22.0",
    "babel-core": "^6.18.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.1",
    "chokidar-cli": "^1.2.0",
    "codecov": "^1.0.1",
    "cross-env": "^3.1.3",
    "eslint": "^3.9.1",
    "eslint-plugin-ava": "^4.0.0",
    "eslint-plugin-compat": "^1.0.2",
    "jquery": "^3.1.1",
    "jsdom": "^9.8.3",
    "maxmin": "^2.1.0",
    "npm-run-all": "^4.1.1",
    "nyc": "^10.0.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "sinon": "^1.17.6",
    "uglify-js": "^2.7.5"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "env": {
      "development": {
        "presets": [
          "es2015"
        ],
        "plugins": [
          "transform-runtime"
        ],
        "sourceMaps": "inline"
      }
    }
  },
  "ava": {
    "require": [
      "babel-core/register",
      "./test/helpers/browser-env.js"
    ],
    "source": [
      "!dist/**/*"
    ]
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "src/lib/compatibility-check.js",
      "test"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR",
    "not ie < 9"
  ]
}
